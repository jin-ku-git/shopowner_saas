                        android:layout_height="@dimen/size_45dp"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="下单人数"
                            android:textColor="@{viewModel.bth_one==0?@color/main_color:@color/main_black_85}"
                            android:textSize="@{viewModel.bth_one==0?@dimen/size_18dp:@dimen/size_14dp}"

