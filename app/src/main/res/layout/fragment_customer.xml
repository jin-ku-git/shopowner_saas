<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="com.youwu.shopowner_saas.ui.finance.fragment.CustomerFragmentViewModel" />
        <import type="android.view.View"/>
        <import type="android.graphics.Typeface"/>
        <variable
            name="viewModel"
            type="CustomerFragmentViewModel" />
    </data>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:binding="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/dp10"
        >
        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/NestedScrollView"
            android:overScrollMode="never"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:focusable="true"
                android:focusableInTouchMode="true"

                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/radius_white"
                    android:padding="@dimen/dp10"
                    >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="顾客分析"
                            android:textStyle="bold"
                            android:textSize="@dimen/dp20"
                            android:layout_centerVertical="true"
                            android:textColor="@color/main_black"
                            />
                        <com.google.android.material.tabs.TabLayout
                            android:id="@+id/tab_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"

                            app:tabGravity="fill"
                            app:tabIndicatorColor="@color/main_color"
                            app:tabIndicatorFullWidth="false"
                            app:tabIndicatorHeight="4dp"
                            app:tabMode="scrollable"
                            app:tabSelectedTextColor="@color/main_color"
                            app:tabTextColor="#545454"
                            app:tabIndicator="@drawable/shape_indicator_radius"
                            app:tabMinWidth="40dip"
                            app:tabContentStart="20dp"
                            app:tabMaxWidth="200dp"
                            app:tabPaddingStart="10dp"
                            app:tabPaddingEnd="10dp"
                            app:tabRippleColor="@null"
                            android:layout_alignParentRight="true"
                            >
                            <com.google.android.material.tabs.TabItem
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="全部顾客" />
                            <com.google.android.material.tabs.TabItem
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="新客" />
                            <com.google.android.material.tabs.TabItem
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="老客" />

                        </com.google.android.material.tabs.TabLayout>

                    </RelativeLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp40"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/dp8"
